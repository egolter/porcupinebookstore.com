
<!DOCTYPE html>
<html lang="en">
<head>
  <title>Porcupine Book Store</title>
  <style>
    .container {
      width: 600px;
      margin: 0 auto;
      text-align:center;
    }
    ul {
      list-style:none;
    }
  </style>
</head>
<body class="container">

    <header>
      <h1>Porcupine Book Store</h1>
    </header>

    <main>
        <div class="row">
            <div class="col-sm-8">
            
                <div class="jumbotron">
                    <h2>Book Catagolgue</h2>
                    <ul id="book-catalogue">
                      <% books.forEach(book => { %>
                        <li><%= book.title %> - <%= book.author %></li> 
                      <% }) %>
                    </ul>
                </div>
                
            </div>
            <div class="col-sm-4">
                
            </div>
        </div>
    </main>

    <footer>
    </footer>
    
</body>
</html>
